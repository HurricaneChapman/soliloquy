{
  "name": "soliloquy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "history": "^4.9.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^4.4.5",
    "react-router": "^3.2.0",
    "react-router-redux": "^4.0.4",
    "react-scripts": "3.0.1",
    "redux": "^3.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "semi": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "max-len": "off",
      "camelcase": "off",
      "react/jsx-indent": [
        "warn",
        4
      ],
      "react/forbid-prop-types": "off",
      "react/jsx-indent-props": [
        "warn",
        4
      ],
      "object-curly-newline": "off",
      "react/jsx-no-bind": "off",
      "no-underscore-dangle": "off"
    },
    "globals": {
      "document": true,
      "window": true,
      "describe": true,
      "it": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
